  }],
  "require": {
    "php": ">=5.3.3",
    "symfony/console": "~2.1",
    "symfony/framework-bundle": "~2.2",
    "symfony/options-resolver": "~2.1",
    "symfony/serializer": "~2.1",
    "kriswallsmith/buzz": "~0.12",
    "biplane/common": "dev-master"
  },
  "autoload": {
    "psr-0": { "Biplane\\YandexDirectBundle": "" }
  },
  "target-dir": "Biplane/YandexDirectBundle",
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
